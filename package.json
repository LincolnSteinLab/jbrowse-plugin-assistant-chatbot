{
  "name": "jbrowse-plugin-assistant-chatbot",
  "version": "0.0.1",
  "private": true,
  "description": "An LLM chatbot assistant widget for JBrowse.",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "homepage": "https://github.com/LincolnSteinLab/jbrowse-plugin-assistant-chatbot#readme",
  "bugs": {
    "url": "https://github.com/LincolnSteinLab/jbrowse-plugin-assistant-chatbot/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LincolnSteinLab/jbrowse-plugin-assistant-chatbot.git"
  },
  "license": "Apache-2.0",
  "author": "Greg Hogue <ghogue@oicr.on.ca>",
  "type": "commonjs",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "setup": "npm-run-all setup:*",
    "setup:file": "ts-node scripts/setup.ts",
    "setup:jbrowse": "npm-run-all setup:jbrowse:*",
    "setup:jbrowse:clean": "rimraf .jbrowse",
    "setup:jbrowse:create": "jbrowse create .jbrowse",
    "clean": "rimraf dist",
    "start": "npm-run-all --sequential clean --parallel start:*",
    "start:watch": "rollup --config --bundleConfigAsCjs --watch --environment JB_NPM:false,NODE_ENV:development",
    "start:server": "x-env serve --cors --listen \\$PLUGIN_PORT .",
    "prebuild": "npm-run-all clean",
    "build": "rollup --config --bundleConfigAsCjs",
    "browse": "npm-run-all jbrowse:*",
    "jbrowse:configure": "shx cp jbrowse_config.json .jbrowse/config.json",
    "jbrowse:serve": "x-env serve --listen \\$JBROWSE_PORT .jbrowse",
    "test": "jest",
    "test:e2e": "x-env start-test \"npm-run-all --parallel start browse\" \"\\$PLUGIN_PORT|\\$JBROWSE_PORT\" \"npm-run-all cypress:run\"",
    "cypress:run": "x-env cypress run --headless --browser chrome --config baseUrl=http://localhost:\\$JBROWSE_PORT",
    "cypress:open": "x-env cypress open --config baseUrl=http://localhost:\\$JBROWSE_PORT",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx --fix src/",
    "prepublishOnly": "npm-run-all test",
    "prepack": "npm-run-all build",
    "postversion": "git push --follow-tags",
    "develop": "npm-run-all --parallel start browse"
  },
  "devDependencies": {
    "@jbrowse/cli": "^3.4.0",
    "@jbrowse/core": "^3.4.0",
    "@jbrowse/development-tools": "^2.2.1",
    "@jbrowse/plugin-linear-genome-view": "^3.6.4",
    "@schemastore/package": "^0.0.10",
    "@tailwindcss/postcss": "^4.1.6",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^15.0.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^22.15.18",
    "@types/react": "^18.2.45",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "chalk": "^4.0.0",
    "cssnano": "^7.0.7",
    "cssnano-preset-advanced": "^7.0.7",
    "cypress": "^13.6.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.6.1",
    "mobx-state-tree": "^5.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-import-css": "^4.0.2",
    "serve": "^14.2.5",
    "shx": "^0.4.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^4.1.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.3.0",
    "x-var": "^2.1.0"
  },
  "overrides": {
    "@jbrowse/development-tools": {
      "@babel/plugin-proposal-class-properties": "@babel/plugin-transform-class-properties@^7.25.9",
      "rollup": "^3.0.0",
      "typescript": "^5.3.3"
    },
    "fsevents": "2.3.2"
  },
  "peerDependencies": {
    "@jbrowse/core": "^3.0.0"
  },
  "dependencies": {
    "@assistant-ui/react": "^0.10.49",
    "@assistant-ui/react-markdown": "^0.10.9",
    "@hookform/resolvers": "^5.2.1",
    "@huggingface/transformers": "^3.6.1",
    "@langchain/anthropic": "^0.3.26",
    "@langchain/community": "^0.3.47",
    "@langchain/core": "^0.3.27",
    "@langchain/google-genai": "^0.2.16",
    "@langchain/langgraph": "^0.4.6",
    "@langchain/ollama": "^0.2.3",
    "@langchain/openai": "^0.6.9",
    "@langchain/textsplitters": "^0.1.0",
    "@mui/material": "^7.1.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "langchain": "^0.3.28",
    "lucide-react": "^0.510.0",
    "mobx-react": "^9.2.0",
    "motion": "^12.23.12",
    "next": "^15.5.2",
    "react-hook-form": "^7.62.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.0",
    "tw-animate-css": "^1.2.9",
    "zod": "^3.25.67",
    "zustand": "^5.0.8"
  },
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "jbrowse-plugin": {
    "name": "AssistantChatbot"
  }
}
