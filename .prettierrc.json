{
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "proseWrap": "always",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^.+\\.css$",
    "<THIRD_PARTY_MODULES>",
    "^(\\.{1,2}|@)\/.*$"
  ],
  "importOrderSeparation": true
}
